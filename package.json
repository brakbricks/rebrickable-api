{
  "name": "@brakbricks/rebrickable-api",
  "version": "0.5.0",
  "description": "A typescript Rebrickable API client",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "docs": "typedoc src/index.ts",
    "lint": "npx eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "format:check": "npm run prettier && npm run lint"
  },
  "keywords": [
    "Lego",
    "API",
    "Rebrickable"
  ],
  "repository": {
    "url": "git+https://github.com/brakbricks/rebrickable-api.git"
  },
  "homepage": "https://brakbricks.github.io/rebrickable-api/",
  "bugs": "https://github.com/brakbricks/rebrickable-api/issues",
  "author": "Brakbricks",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@jest/globals": "^29.7.0",
    "@stylistic/eslint-plugin": "^2.7.2",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "tsup": "^8.2.4",
    "typedoc": "^0.26.5",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "eslint-plugin-prettier": "^5.2.1"
  }
}
